RULE-NAME-PREFIX FERC

assert F6.200.1 satisfied 
$rule_id = rule-name();
if ($waivedSchedules intersect $scheduleLookup[(rule-name().split('.'))[3]]).length > 0
        skip
else

if set('6-Q').contains([covered @concept.local-name ='FormType'])
        skip
else
 $sum1 = {@ReceivablesFromAffiliatedCompanies @ReceivablesFromAffiliatedCompaniesAxis=none @period=$currentInstant}; 
 $addend = sum(list({nonils @ReceivablesFromAffiliatedCompanies @ReceivablesFromAffiliatedCompaniesAxis=* @period=$currentInstant})#breakdown);
tolerance_for_decimals($sum1,$addend,$tolerance_factor)  
