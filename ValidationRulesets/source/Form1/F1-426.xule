assert F1.426.11 satisfied 
$rule_id = rule-name();

if set('1-F','1/3-Q').contains([covered @concept.local-name ='FormType'])
        skip
else
	$codes = list('Transmission','Distribution','Generation')
	if {@SubstationCharacterDescription @SubstationsAxis=*}#code not in $codes
                 true
   	 else
   	 	skip
message 
"The substation type with a value of {$code} has been used. This is an invalid code. Substation types must be one of the following  {$codes.join(', ')}.

Element : {$code.concept.name}
Code Period : {$code.period}
Code Value : {$code}
Code Dimension: {$code.dimensions.join(',','=')}

Rule Id:{$rule_id}"
severity error 
rule-focus $code

