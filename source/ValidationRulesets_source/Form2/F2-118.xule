assert F2.118.6 satisfied 
$rule_id = rule-name();
if ($waivedSchedules intersect $scheduleLookup[(rule-name().split('.'))[3]]).length > 0
        skip
else
if set('2/3-Q').contains([covered @concept.local-name ='FormType'])
